/**index.wxss**/
.intro_background{
  z-index: 1001;
  background-color: rgba(0, 0, 0, 0.6);
  position: absolute;
  height: 100%;
  width: 100%;  
}

.know_more{
  z-index: 1002;
  color: white;
  border: none;
  position: absolute;
  bottom: 150rpx;
  width: 100%;
  text-align: center;
  text-decoration-line: underline;
}

.intro_agree{
  z-index: 1002;
  position: fixed;
  bottom: 20rpx;
  width: 100%;
  background-color: #ff6500;
  color: white;
  border: none;
}

.search_tab{
  background-color: transparent;
  display: flex;
  padding: 5rpx;
}

input.search_input{
  background-color: #eee;
  display:flex;
  flex-direction: column;
  padding: 3rpx;
  line-height: 3rpx;
  width: 520rpx;
  z-index: 10;
  border-radius: 25rpx;
  margin-left: 80rpx;
  font-size: 75%;
  padding-left: 30rpx;
}

.app_instruction{
  width: 140rpx;
  height: 45rpx;
  font-size: 55%;
  letter-spacing: 1rpx;
  margin-top: 35rpx;
  margin-left: 100rpx;
  border-bottom-left-radius: 30rpx;
  border-top-left-radius: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  color: #e0e0da;
}

.search_map{
  display: flex;
  flex-direction: column;
  width: 750rpx;
  height: 90%;
  position: absolute;
  bottom: 0;
  z-index: 1;

}

button.search{
  height: 45rpx;
  border: #fff;
  z-index: 10;
  border-radius: 50rpx;
  font-size: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 102rpx;
  letter-spacing: 4rpx;
}

input-placeholder.search_placeholder{
  color: #979496;
}

.top_bar{
  background-color: #ff6500;
  height: 135rpx;
  display: flex;
  flex-direction: row;
  z-index: 10;
}

.userinfo {
  align-items: center;
}

.userinfo-avatar {
  width: 75rpx;
  height: 75rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.nearby_bar{
  background-color: #d85600;
  margin-left: 65rpx;
  height: 100rpx;
  width: 385rpx;
  border-radius: 125rpx;
  border-style: solid;
  border-color: #e0e0da;
  border-width: 0.8rpx;
  display: flex;
  flex-direction: row;
}

button.nearby_all{
  width: 210rpx;
  border: none;
  font-size: 60%;
  border-radius: 50rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;

}

button.nearby_rent{
  width: 210rpx;
  border: none;
  font-size: 60%;
  border-radius: 50rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.locate_current{
  background-image: url('data:image/png;base64,');
  background-size: 100%;
  background-repeat: no-repeat;
  color: transparent;
  border: transparent;
  z-index: 1000;
  position: absolute;
  height: 75rpx;
  width: 75rpx;
  bottom: 150rpx;
  right: 50rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  border-radius: 50rpx;
}

.scan_qr{
  background-image: url("data:image/jpeg;base64,");
  background-size: 190%;
  background-position: center;
  color: transparent;
  border: none;
  height: 190rpx;
  width: 190rpx;
  z-index: 1000;
  margin-top: 780rpx;
  border-radius: 100rpx;
}

.current_points{
  z-index: 10;
  bottom: 0;
  width: 60%;
  font-size: 75%;
  border-radius: 50rpx;
  color: #ff6500
}